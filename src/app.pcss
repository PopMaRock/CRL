@tailwind base;
@tailwind components;
@tailwind utilities;

/* === StoryText === */
.font-smoothing {
	-moz-osx-font-smoothing: grayscale;
	-webkit-font-smoothing: antialiased !important;
	-moz-font-smoothing: antialiased !important;
	text-rendering: optimizeLegibility !important;
}
.chat-text {
	max-width: 65ch;
	font-size: 1.35em;
	letter-spacing: 0.03em;
	hyphens: auto;
  }
  @media screen and (min-width: 700px) {
    * ::-webkit-scrollbar,::-webkit-scrollbar {
        width:2px!important;
        border-radius: 999px;
        height: 0!important
    }

    * ::-webkit-scrollbar-track,::-webkit-scrollbar-track {
        background: transparent!important
    }

    * ::-webkit-scrollbar-thumb,::-webkit-scrollbar-thumb {
        border-radius: 999px;
        background: hsla(0,0%,100%,.5)!important;
        height: 16px!important
    }

    * ::-webkit-scrollbar-thumb:hover,::-webkit-scrollbar-thumb:hover {
        background: #fff!important
    }
}
/* === Horizontal Rules === */

hr:not(.divider) {
	@apply border-surface-300-600-token block border-t border-solid;
}
.divider-vertical {
	@apply border-surface-300-600-token mx-auto inline-block min-h-[10px] border-l border-solid;
}
@keyframes button-pop {
	0% {
		transform: scale(0.93);
	}
	40% {
		transform: scale(1.02);
	}
	100% {
		transform: scale(1);
	}
}

.red-span {
	color: rgb(228, 55, 55);
	font-weight: 400;
}
/* Shite scraped up from other pages */
.dotty {
	position: absolute;
	top: 0;
}
/*FROM +page.svelte*/
.main-game {
	display: flex;
	flex-direction: column;
	justify-content: center;
	gap: 3rem;
}
.game-master {
	width: 70%;
	height: 25%;
	line-height: 1.8;
	background-color: #0d0d0db3;
	backdrop-filter: blur(24px);
	margin-inline: auto;
	padding: 0.5rem 0.9rem;
	border-radius: 1rem;
	font-size: 1.4rem;
	color: #eee;
	overflow: auto;
}

.game-controls {
	display: flex;
	width: 70%;
	margin-inline: auto;
	align-items: center;
	justify-content: space-between;
	gap: 2rem;
	height: 30%;
}

.choices {
	width: 100%;
	height: 100%;
}
/* responsive */
@media (orientation: portrait) {
	.game-master {
		width: 85%;
	}
	.main-game {
		height: 100vh;
	}
	.game-controls {
		display: grid;
		grid-template-columns: 1fr 1fr;
		width: 85%;
		height: 40%;
	}
	.choices {
		grid-column-start: 1;
		grid-column-end: 3;
		grid-row-end: 1;
		grid-row-end: 2;
		padding-bottom: 2rem;
	}
}
